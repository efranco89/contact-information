<!DOCTYPE html>
<html>
  <head>
    <title>Contact Importer</title>
    <%= csrf_meta_tags %>
    <%= csp_meta_tag %>

    <%= stylesheet_link_tag 'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_pack_tag 'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= stylesheet_pack_tag 'application', 'data-turbolinks-track': 'reload' %>
  </head>

  <body>
    <!-- logs_contact_infos_path, method: :delete -->
    <%= link_to destroy_user_session_path, method: :delete, class: 'btn btn-info m-2' do %>
      <i class="fas fa-sign-out-alt"></i>&nbsp;Logout
    <% end %>
    <p class="notice"><%= notice %></p>
    <p class="alert"><%= alert %></p>
    <%= yield %>
  </body>
</html>
